<UserControl x:Class="Prevu.Drawings.Machines.Misc.DashedLine"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Prevu.Drawings.Machines.Misc"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
	<Grid>
		<Line Stroke="Black" StrokeDashArray="2" X2="1" StrokeThickness="2" 
					StrokeDashCap="Round" StrokeEndLineCap="Round" 
          StrokeStartLineCap="Round"  Stretch="Fill" Margin="0 0"/>
	</Grid>
</UserControl>
