<UserControl x:Class="Prevu.Drawings.Machines.EXC_Air"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Prevu.Drawings.Machines"
             mc:Ignorable="d" 
             Height="44" Width="100">
    <Viewbox Stretch="Uniform">
        <Canvas Height="44" Width="100">
            <local:VRC_WaterCollectingTank Height="44" Width="100"/>
            <local:Agitator Height="19" Width="52" Canvas.Top="25" Canvas.Left="23" HorizontalAlignment="Left" VerticalAlignment="Top"/>
            <Rectangle Height="8" Width="100" RenderTransformOrigin="0.5,0.5" Fill="#FF616161" RadiusX="2" RadiusY="2"/>
        </Canvas>
    </Viewbox>
</UserControl>
