<UserControl x:Class="Prevu.Drawings.Misc.DashedLineVertical"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Prevu.Drawings.Misc"
             mc:Ignorable="d" Height="100" Width="4">
  <Grid>
    <Line Stroke="Black" StrokeDashArray="4" X2="0" Y2="1" StrokeThickness="4" StrokeDashCap="Round" StrokeEndLineCap="Round" StrokeStartLineCap="Round"  Stretch="Fill" />
  </Grid>
</UserControl>
