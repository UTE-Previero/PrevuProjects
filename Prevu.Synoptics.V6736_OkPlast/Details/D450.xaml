<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Prevu.Synoptics"
			 xmlns:Machines="clr-namespace:Prevu.Drawings.Machines;assembly=Prevu.Drawings" 
             xmlns:Labels="clr-namespace:Prevu.UIComponents.Labels;assembly=Prevu.UIComponents" 
             xmlns:lex="http://wpflocalizeextension.codeplex.com" 
             x:Class="Prevu.Synoptics.D450" 
             mc:Ignorable="d" 
             
             d:Background="{DynamicResource MaterialDesignPaper}">
    <Canvas Height="308" Width="491">
        <Labels:Pipe Color="Water" Width="134" StatusComponent="450-21-CV1" RenderTransformOrigin="0.5,0.5" Canvas.Left="290" Canvas.Top="116" HorizontalAlignment="Left" VerticalAlignment="Center">
            <Labels:Pipe.RenderTransform>
                <TransformGroup>
                    <RotateTransform Angle="90"/>
                </TransformGroup>
            </Labels:Pipe.RenderTransform>
        </Labels:Pipe>
        <Labels:Pipe Width="166" RenderTransformOrigin="0.497,0.467" Color="Chemicals" StatusComponent="451-31-CV1" Canvas.Left="146" Canvas.Top="104" Text="{lex:Loc ADDITIVE}" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        <Labels:Pipe Width="98" RenderTransformOrigin="0.497,0.467" Color="Chemicals" StatusComponent="451-31-CV1" Canvas.Left="262" Canvas.Top="158" HorizontalAlignment="Center" VerticalAlignment="Top">
            <Labels:Pipe.RenderTransform>
                <TransformGroup>
                    <RotateTransform Angle="90"/>
                </TransformGroup>
            </Labels:Pipe.RenderTransform>
        </Labels:Pipe>
        <Labels:Pipe Color="Vapor" Width="74" StatusComponent="450-22-CV1" RenderTransformOrigin="0.5,0.5" Canvas.Left="159" Canvas.Top="229" HorizontalAlignment="Left" VerticalAlignment="Center"/>
        <Machines:V01_VRC_WaterTank Canvas.Left="200" Canvas.Top="166" Height="132" Width="244" ComponentName="450-41-LT1" TrendName="LEVEL" HorizontalAlignment="Left" VerticalAlignment="Center"/>
        <Machines:CV_Valve_001 Height="50" Width="50" Color="Water" RenderTransformOrigin="0.51,0.539" Canvas.Left="333" Canvas.Top="8" HorizontalAlignment="Left" VerticalAlignment="Center">
            <Machines:CV_Valve_001.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-90"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Machines:CV_Valve_001.RenderTransform>
        </Machines:CV_Valve_001>
        <Machines:WaterPump Height="46" Width="44" RenderTransformOrigin="0.5,0.5" Color="Chemicals" Canvas.Left="126" Canvas.Top="91" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Machines:CV_Valve_001 Height="50" Width="50" Color="Steam" RenderTransformOrigin="0.51,0.539" Canvas.Left="122" Canvas.Top="207" HorizontalAlignment="Left" VerticalAlignment="Center">
            <Machines:CV_Valve_001.RenderTransform>
                <TransformGroup>
                    <ScaleTransform ScaleX="-1"/>
                </TransformGroup>
            </Machines:CV_Valve_001.RenderTransform>
        </Machines:CV_Valve_001>
        <Rectangle Height="2" Stroke="Black" Width="30" Canvas.Left="363" Canvas.Top="104" HorizontalAlignment="Left" VerticalAlignment="Center"/>
        <Ellipse Height="16" Stroke="#FFBDBDBD" Width="16" OpacityMask="#FFBDBDBD" Fill="{DynamicResource {x:Static SystemColors.AppWorkspaceBrushKey}}" Canvas.Left="349" Canvas.Top="97" HorizontalAlignment="Left" VerticalAlignment="Center"/>
        <Labels:DeviceLabel ComponentName="450-41-LT1" Canvas.Left="222" Canvas.Top="207" HorizontalAlignment="Left" VerticalAlignment="Center"/>
        <Labels:DeviceLabel ComponentName="450-44-FT1" Canvas.Left="382" Canvas.Top="80" HorizontalAlignment="Left" VerticalAlignment="Center"/>
        <Labels:DeviceLabel ComponentName="450-21-CV1" Canvas.Left="382" Canvas.Top="12" HorizontalAlignment="Left" VerticalAlignment="Center"/>
        <Labels:DeviceLabel ComponentName="451-31-CV1" Canvas.Left="18" Canvas.Top="90" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        <Labels:DeviceLabel ComponentName="450-22-CV1" Canvas.Left="18" Canvas.Top="207" HorizontalAlignment="Left" VerticalAlignment="Center"/>
        <Labels:DeviceLabel ComponentName="450-42-TT1" Canvas.Left="333" Canvas.Top="207" HorizontalAlignment="Left" VerticalAlignment="Center"/>
    </Canvas>
</UserControl>
