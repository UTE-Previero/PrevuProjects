<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Prevu.Synoptics"
						 xmlns:Machines="clr-namespace:Prevu.Drawings.Machines;assembly=Prevu.Drawings" 
             xmlns:Labels="clr-namespace:Prevu.UIComponents.Labels;assembly=Prevu.UIComponents" 
             xmlns:Separation="clr-namespace:Prevu.Drawings.Machines.Separation;assembly=Prevu.Drawings" x:Class="Prevu.Synoptics.D883" 
             mc:Ignorable="d" 
             Height="600" Width="600" d:Background="{DynamicResource MaterialDesignPaper}">
    <Grid>
        <Machines:SCW_Flex_Verticale_DX HorizontalAlignment="Left" Margin="0,-350,0,220" RenderTransformOrigin="0.5,0.5" Width="606" Loaded="SCW_Flex_Verticale_DX_Loaded">
