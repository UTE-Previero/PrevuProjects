                <TransformGroup>
                    <ScaleTransform ScaleX="-1"/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </Machines:SCW_Flex_Verticale_DX.RenderTransform>
        </Machines:SCW_Flex_Verticale_DX>
        <Labels:SignalLabel HorizontalAlignment="Left" Margin="165,240,0,0" VerticalAlignment="Top" ComponentName="810-61-LEV" SignalName="LSHA" FlowDirection="RightToLeft"/>
        <Labels:SignalLabel HorizontalAlignment="Left" Margin="234,304,0,0" VerticalAlignment="Top" ComponentName="810-61-LEV" SignalName="LSH" FlowDirection="RightToLeft" />
        <Labels:DeviceLabel HorizontalAlignment="Left" 
         Margin="340,314,0,0" 
         VerticalAlignment="Top" 
         ComponentName="883-61-LEV"/>
