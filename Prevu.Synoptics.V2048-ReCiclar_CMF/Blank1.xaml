<UserControl x:Class="Prevu.Synoptics.Blank1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Prevu.Synoptics"
             xmlns:Machines="clr-namespace:Prevu.Drawings.Machines;assembly=Prevu.Drawings" 
             xmlns:Labels="clr-namespace:Prevu.UIComponents.Labels;assembly=Prevu.UIComponents" 
						 xmlns:lex="http://wpflocalizeextension.codeplex.com"
						 lex:ResxLocalizationProvider.DefaultAssembly="Prevu.Localization"
						 lex:ResxLocalizationProvider.DefaultDictionary="Resources"
						 mc:Ignorable="d"
             Height="840" Width="1520" d:Background="{DynamicResource MaterialDesignPaper}">
    <Grid>
        <Labels:DeviceLabel HorizontalAlignment="Left" 
                        Margin="59,35,0,0" 
                        VerticalAlignment="Top" 
                        ComponentName="230-01-MA1"/>

        <Labels:JumpLabel HorizontalAlignment="Left" 
                      Margin="59,103,0,0" 
                      VerticalAlignment="Top" 
                      JumpFromLabel="230-01-MA1" 
                      JumpToLabel="230-01-MA1"/>

        <Labels:Pipe HorizontalAlignment="Left" 
                 Margin="54,177,0,0" 
                 VerticalAlignment="Top" 
                 Icon="Right" 
                 StatusComponent="230-01-MA1" />

        <Labels:SignalLabel HorizontalAlignment="Left" 
                        Margin="30,231,0,0" 
                        VerticalAlignment="Top" 
                        Width="148" 
                        ComponentName="230-01-MA1" 
                        SignalName="230-01-ZS1"/>
        <Machines:Buffer_belt HorizontalAlignment="Left" Margin="272,39,0,0" VerticalAlignment="Top" Height="198" Width="290" RenderTransformOrigin="0.5,0.5">
            <Machines:Buffer_belt.RenderTransform>
                <TransformGroup>
                    <ScaleTransform ScaleX="-1"/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </Machines:Buffer_belt.RenderTransform>
        </Machines:Buffer_belt>
        <Machines:NT_ConveyorBelt HorizontalAlignment="Left" Margin="610,208,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5">
            <Machines:NT_ConveyorBelt.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Machines:NT_ConveyorBelt.RenderTransform>
        </Machines:NT_ConveyorBelt>
        <Machines:WCE_WashingCentrifuge HorizontalAlignment="Left" Margin="417,35,0,0" VerticalAlignment="Top"/>
        <Machines:SCW_ScrewConveyor_straight HorizontalAlignment="Left" Margin="288,237,0,0" VerticalAlignment="Top"/>
        <Machines:SCW_ScrewConveyor_inclined HorizontalAlignment="Left" Margin="522,260,0,0" VerticalAlignment="Top"/>
        <Machines:VB_VibratingScreen HorizontalAlignment="Left" Margin="635,73,0,0" VerticalAlignment="Top"/>
        <Machines:VRC_WaterCollectingTank HorizontalAlignment="Center" Margin="0,272,0,0" VerticalAlignment="Top"/>
        <Machines:Manual_selection HorizontalAlignment="Left" Margin="95,295,0,0" VerticalAlignment="Top" Height="256" Width="181"/>
        <Machines:VT_BlowerCentrifuge_down HorizontalAlignment="Left" Margin="438,446,0,0" VerticalAlignment="Top"/>
        <Machines:DCE_DryingCentrifuge HorizontalAlignment="Left" Margin="710,408,0,0" VerticalAlignment="Top"/>
        <Machines:VSP_SeparationTank HorizontalAlignment="Left" Margin="288,423,0,0" VerticalAlignment="Top"/>
        <Machines:DV_Diverter_Y HorizontalAlignment="Left" Margin="596,379,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5">
            <Machines:DV_Diverter_Y.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="180"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Machines:DV_Diverter_Y.RenderTransform>
        </Machines:DV_Diverter_Y>
        <Machines:GR_001 HorizontalAlignment="Left" Margin="833,89,0,0" VerticalAlignment="Top"/>
    </Grid>
</UserControl>
