<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Prevu.Synoptics"
			 xmlns:Machines="clr-namespace:Prevu.Drawings.Machines;assembly=Prevu.Drawings" 
             xmlns:Labels="clr-namespace:Prevu.UIComponents.Labels;assembly=Prevu.UIComponents" 
             xmlns:lex="http://wpflocalizeextension.codeplex.com" 
             x:Class="Prevu.Synoptics.D450" 
             mc:Ignorable="d" 
             
             d:Background="{DynamicResource MaterialDesignPaper}">
    <Canvas Height="308" Width="431">
        <Labels:Pipe Color="Water" Width="134" StatusComponent="450-21-CV1" RenderTransformOrigin="0.5,0.5" Canvas.Left="230" Canvas.Top="116" HorizontalAlignment="Center" VerticalAlignment="Top">
            <Labels:Pipe.RenderTransform>
                <TransformGroup>
                    <RotateTransform Angle="90"/>
                </TransformGroup>
            </Labels:Pipe.RenderTransform>
        </Labels:Pipe>
        <Labels:Pipe Width="166" RenderTransformOrigin="0.497,0.467" Color="Chemicals" StatusComponent="451-31-CV1" Canvas.Left="59" Canvas.Top="81" Text="{lex:Loc ADDITIVE}" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        <Labels:Pipe Width="98" RenderTransformOrigin="0.497,0.467" Color="Chemicals" StatusComponent="451-31-CV1" Canvas.Left="175" Canvas.Top="135" HorizontalAlignment="Center" VerticalAlignment="Top">
            <Labels:Pipe.RenderTransform>
                <TransformGroup>
                    <RotateTransform Angle="90"/>
                </TransformGroup>
            </Labels:Pipe.RenderTransform>
        </Labels:Pipe>
        <Machines:V01_VRC_WaterTank Canvas.Left="140" Canvas.Top="166" Height="132" Width="244" ComponentName="450-41-LT1" TrendName="LEVEL" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        <Machines:CV_Valve_001 Height="50" Width="50" Color="Water" RenderTransformOrigin="0.51,0.539" Canvas.Left="273" Canvas.Top="8" HorizontalAlignment="Center" VerticalAlignment="Top">
            <Machines:CV_Valve_001.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-90"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Machines:CV_Valve_001.RenderTransform>
        </Machines:CV_Valve_001>
        <Rectangle Height="2" Stroke="Black" Width="30" Canvas.Left="303" Canvas.Top="104" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        <Ellipse Height="16" Stroke="#FFBDBDBD" Width="16" OpacityMask="#FFBDBDBD" Fill="{DynamicResource {x:Static SystemColors.AppWorkspaceBrushKey}}" Canvas.Left="289" Canvas.Top="97" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        <Labels:DeviceLabel ComponentName="450-41-LT1" Canvas.Left="217" Canvas.Top="207" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        <Labels:DeviceLabel ComponentName="450-44-FT1" Canvas.Left="322" Canvas.Top="80" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        <Labels:DeviceLabel ComponentName="450-21-CV1" Canvas.Left="322" Canvas.Top="12" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        <Machines:WaterPump Height="46" Width="44" RenderTransformOrigin="0.5,0.5" Color="Chemicals" Canvas.Left="34" Canvas.Top="68" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        <Labels:DeviceLabel ComponentName="451-31-CV1" Canvas.Left="11" Canvas.Top="13" HorizontalAlignment="Center" VerticalAlignment="Top"/>
    </Canvas>
</UserControl>
