        <Labels:DeviceLabel VerticalAlignment="Top" Canvas.Top="344" Canvas.Left="104" HorizontalAlignment="Left" ComponentName="085-01-MA1"/>
        <Labels:DeviceLabel VerticalAlignment="Top" Canvas.Top="81" Canvas.Left="23" HorizontalAlignment="Left" ComponentName="085-02-MA1"/>
        <Labels:DeviceLabel VerticalAlignment="Top" Canvas.Top="229" Canvas.Left="171" HorizontalAlignment="Left" ComponentName="085-41-LT1"/>
        <TextBlock TextWrapping="Wrap" TextAlignment="Center" FontSize="20" d:Foreground="White" Text="{lex:Loc Washing}" Canvas.Left="119" Canvas.Top="10" HorizontalAlignment="Left" VerticalAlignment="Top" Width="149"/>
    </Canvas>
</UserControl>
