            <Labels:Pipe.RenderTransform>
                <TransformGroup>
                    <RotateTransform Angle="90"/>
                </TransformGroup>
            </Labels:Pipe.RenderTransform>
        </Labels:Pipe>
        <Machines:WaterPump Width="46" VerticalAlignment="Top" Canvas.Top="287" RenderTransformOrigin="0.5,0.5" Canvas.Left="125" Height="44" HorizontalAlignment="Left" Color="Water" BorderBrush="Black">
            <Machines:WaterPump.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-90"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Machines:WaterPump.RenderTransform>
        </Machines:WaterPump>
        <Rectangle Height="6" Stroke="Black" Width="44" Fill="#FFBDBDBD" RenderTransformOrigin="0.5,0.5" HorizontalAlignment="Left" Canvas.Left="127" Canvas.Top="51" VerticalAlignment="Top"/>
