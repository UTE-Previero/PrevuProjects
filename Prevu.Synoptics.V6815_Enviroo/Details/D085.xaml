<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Prevu.Synoptics"
			 xmlns:Machines="clr-namespace:Prevu.Drawings.Machines;assembly=Prevu.Drawings" 
             xmlns:Labels="clr-namespace:Prevu.UIComponents.Labels;assembly=Prevu.UIComponents" 
             xmlns:lex="http://wpflocalizeextension.codeplex.com" 
             x:Class="Prevu.Synoptics.D085" 
             mc:Ignorable="d" 
             
             d:Background="{DynamicResource MaterialDesignPaper}">
    <Canvas Height="410" Width="387">
        <Machines:V01_PSP_Pit_wAgitator Width="280" VerticalAlignment="Top" Canvas.Top="115" TrendName="LEVEL" Canvas.Left="31" Height="229" HorizontalAlignment="Left" ComponentName="085-41-LT1"/>
